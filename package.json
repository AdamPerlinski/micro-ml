{
  "name": "micro-ml-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo run build",
    "build:wasm": "cd crates/micro-ml-core && wasm-pack build --target web --out-dir ../../packages/micro-ml/wasm",
    "test": "turbo run test",
    "test:rust": "cd crates/micro-ml-core && cargo test",
    "bench": "turbo run bench",
    "clean": "turbo run clean && rm -rf node_modules",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  }
}
